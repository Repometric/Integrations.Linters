{
  "version": "1.2.2-*",
  "buildOptions": {
    "outputName": "cli",
    "debugType": "portable",
    "emitEntryPoint": true,
    "compile": {
      "exclude": [ "linterhub" ]
    }
  },
  "publishOptions": {
    "include": ["Config", "linterhub"]
  },

  "dependencies": {
    "NETStandard.Library": {
      "type": "platform",
      "version": "1.6.0"
    },
    "Mono.Options": "4.4.0",
    "System.Diagnostics.Process": "4.1.0",
    "engine": "1.1.2-*"
  },

  "frameworks": {
    "netstandard1.6": {
       "imports": "dnxcore50"
    }
  }
}
